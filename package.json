{
  "name": "circleci-aws",
  "version": "1.0.0",
  "repository": "git@github.com:ChristianRich/circleci-aws.git",
  "license": "ISC",
  "scripts": {
    "start": "npm run build && node build/server/index.js",
    "build": "rm -rf ./build && npm run build:server",
    "build:server": "rm -rf ./build/server && babel -d ./build/server ./server -s --copy-files",
    "test": "echo No tests configured"
  },
  "engines": {
    "node": ">=8.11.3",
    "npm": ">=6.2.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "max-len": [
        "error",
        {
          "code": 150
        }
      ],
      "no-restricted-syntax": [
        "off",
        {
          "selector": "for..of"
        }
      ],
      "no-underscore-dangle": 0,
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "prefer-destructuring": [
        "error",
        {
          "AssignmentExpression": {
            "array": false,
            "object": false
          }
        }
      ]
    }
  },
  "eslintIgnore": [
    "/build"
  ],
  "babel": {
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    },
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8.9.4"
          },
          "include": [
            "transform-async-to-generator",
            "es7.object.values",
            "es7.object.entries"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "winston": "^3.0.0",
    "winston-daily-rotate-file": "^3.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  }
}
